    "eslint-plugin-node": "11.1.0",
    "jquery": "3.6.0",
    "jquery.scrollto": "2.1.3",
    "laravel-mix": "6.0.43",
    "postcss-scss": "4.0.3",
    "prettier": "2.6.2",
    "prismjs": "1.28.0",
    "resolve-url-loader": "5.0.0",
    "sass": "1.50.1",
    "sass-loader": "12.6.0",
    "stylelint": "14.7.1",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-scss": "4.2.0"
  },
  "dependencies": {
    "@absolunet/brand-guidelines": "^1.1.0",
