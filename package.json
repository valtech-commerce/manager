    "@absolunet/fss": "^1.7.0",
    "@absolunet/joi": "^0.3.0",
    "@absolunet/private-registry": "^2.0.0",
    "@absolunet/terminal": "^3.2.0",
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "babel-loader": "^8.2.5",
    "chalk": "^5.0.1",
